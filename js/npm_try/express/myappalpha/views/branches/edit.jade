extends ../layout

block content
  h1= title

  form(action="/branches/edit" method="post")
    table.table
      tr
        td
          label Branch Name
        td
          input(type="text" name="branch_name" )
      tr
        td
          label Branch Owner
        td
          input(type="text" name="branch_owner" )
      tr
        td
          label Branch From
        td
          input(type="text" name="branch_from" )
      tr
        td
          label Created Date
        td
          input(type="text" name="created_date" )
      tr
        td
          label Expire Date
        td
          input(type="text" name="expire_date" )
      tr
        td
          input(type="submit" value="submit")


